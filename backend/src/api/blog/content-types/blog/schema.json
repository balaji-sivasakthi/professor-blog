{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "minLength": 1,
      "default": "title",
      "unique": true
    },
    "Description": {
      "type": "richtext",
      "default": "description",
      "required": true,
      "minLength": 1
    },
    "Rating": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1
    },
    "Comment": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "Author": {
      "type": "string",
      "required": true
    },
    "Designation": {
      "type": "string",
      "required": true
    }
  }
}
